{
  "name": "iconfont-for-human",
  "displayName": "iconfont-for-human",
  "description": "make iconfont use again",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LouisGoTop/iconfont-for-human.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact",
    "onLanguage:css",
    "onCustomEditor:font.preview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "iconfont-for-human.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "iconfont-for-human.copyIconName",
        "title": "ifh: 复制图标名称"
      },
      {
        "command": "iconfont-for-human.copyIconCode",
        "title": "ifh: 复制图标 Unicode"
      }
    ],
    "menus": {
      "editor/lineNumber/context": [
        {
          "command": "iconfont-for-human.copyIconName",
          "group": "iconfont@1"
        },
        {
          "command": "iconfont-for-human.copyIconCode",
          "group": "iconfont@2"
        }
      ]
    },
    "customEditors": [
      {
        "viewType": "font.preview",
        "displayName": "Font Preview",
        "selector": [
          {
            "filenamePattern": "*.ttf"
          },
          {
            "filenamePattern": "*.otf"
          },
          {
            "filenamePattern": "*.woff"
          },
          {
            "filenamePattern": "*.woff2"
          },
          {
            "filenamePattern": "*.eot"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/pako": "^2.0.3",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "fonteditor-core": "^2.4.1",
    "node-fetch": "^3.3.2",
    "pako": "^2.1.0"
  }
}
